{
  "name": "windshieldjs",
  "version": "3.0.0",
  "description": "An Enterprise Rendering Plugin for Hapi.js",
  "repository": "https://github.com/carsdotcom/windshieldjs.git",
  "main": "lib/index.js",
  "scripts": {
    "test": "node --es_staging node_modules/.bin/_mocha \"test/**/*.js\"",
    "watch": "node --es_staging node_modules/.bin/_mocha --watch --bail",
    "coverage": "node --es_staging node_modules/.bin/istanbul cover node_modules/.bin/_mocha",
    "lint": "eslint ./lib",
    "docs": "hugo -s ./hugo -d ../docs"
  },
  "author": "Mac Heller-Ogden",
  "license": "Apache-2.0",
  "dependencies": {
    "bluebird": "^3.0.5",
    "boom": "3.0.0",
    "handlebars": "^4.0.5",
    "hoek": "3.0.1",
    "iniparser": "^1.0.5",
    "inquirer": "^0.12.0",
    "joi": "7.0.0",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "mkdirp": "^0.5.1",
    "stream-conflict": "^0.0.1"
  },
  "devDependencies": {
    "async": "^1.5.0",
    "chai": "^3.5.0",
    "eslint": "^1.10.3",
    "hapi": "^11.1.2",
    "istanbul": "^0.4.0",
    "mocha": "^2.3.4",
    "pre-commit": "^1.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "vision": "^4.0.1"
  },
  "pre-commit": [
    "test",
    "lint"
